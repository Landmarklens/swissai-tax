# SwissAI Tax - Environment Variables Template
# Copy this file to .env.local for local development

# Stripe Configuration
# Get from: aws ssm get-parameter --name '/swissai-tax/stripe/publishable-key' --region us-east-1
REACT_APP_STRIPE_PUBLISHABLE_KEY=

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_SUBSCRIPTIONS=true

# Optional: Analytics
# REACT_APP_GA_TRACKING_ID=
# REACT_APP_GOOGLE_TAG_MANAGER_ID=

# Note: For production deployment, set these via your deployment platform's
# environment variable configuration, NOT in .env files.
# See DEPLOYMENT_CONFIG.md for details.
