{
  "ServiceName": "swissai-tax-api",
  "SourceConfiguration": {
    "CodeRepository": {
      "RepositoryUrl": "https://github.com/trustorno/swissai-tax",
      "SourceCodeVersion": {
        "Type": "BRANCH",
        "Value": "main"
      },
      "CodeConfiguration": {
        "ConfigurationSource": "REPOSITORY"
      }
    },
    "AutoDeploymentsEnabled": false
  },
  "InstanceConfiguration": {
    "Cpu": "0.25 vCPU",
    "Memory": "0.5 GB",
    "InstanceRoleArn": ""
  },
  "HealthCheckConfiguration": {
    "Protocol": "HTTP",
    "Path": "/health",
    "Interval": 10,
    "Timeout": 5,
    "HealthyThreshold": 1,
    "UnhealthyThreshold": 5
  },
  "Tags": [
    {
      "Key": "Project",
      "Value": "SwissAI-Tax"
    },
    {
      "Key": "Environment",
      "Value": "Production"
    }
  ]
}